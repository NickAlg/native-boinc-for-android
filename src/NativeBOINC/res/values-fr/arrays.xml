<resources>

    <!-- Screen rotation mode -->
    <string-array name="prefScreenOrientationDisplay">
        <item>Réglage système</item>
        <item>Forcer en mode portrait</item>
        <item>Forcer en mode paysage</item>
        <item>Rotation automatique</item>
    </string-array>
    <string-array name="prefScreenOrientationValues">

        <!-- values from android.content.pm.ActivityInfo: -->
        <item>-1</item> <!-- SCREEN_ORIENTATION_UNSPECIFIED -->
        <item>1</item> <!-- SCREEN_ORIENTATION_PORTRAIT -->
        <item>0</item> <!-- SCREEN_ORIENTATION_LANDSCAPE -->
        <item>4</item> <!-- SCREEN_ORIENTATION_SENSOR -->
    </string-array>

    <!-- Automatic updates of display -->
    <string-array name="prefAutoUpdatesDisplay">
        <item>Disabled</item>
        <item>1s</item>
        <item>5s</item>
        <item>10s</item>
        <item>30s</item>
        <item>60s</item>
    </string-array>
    <string-array name="prefAutoUpdatesValues">
        <item>0</item>
        <item>1</item>
        <item>5</item>
        <item>10</item>
        <item>30</item>
        <item>60</item>
    </string-array>

    <!-- screen lock update -->
    <string-array name="prefScreenLockUpdateDisplay">
        <item>1s</item>
        <item>5s</item>
        <item>10s</item>
        <item>30s</item>
        <item>60s</item>
    </string-array>
    <string-array name="prefScreenLockUpdateValues">
        <item>1</item>
        <item>5</item>
        <item>10</item>
        <item>30</item>
        <item>60</item>
    </string-array>

    <!-- widget update -->
    <string-array name="prefWidgetUpdateDisplay">
        <item>1s</item>
        <item>5s</item>
        <item>10s</item>
        <item>30s</item>
        <item>60s</item>
    </string-array>
    <string-array name="prefWidgetUpdateValues">
        <item>1</item>
        <item>5</item>
        <item>10</item>
        <item>30</item>
        <item>60</item>
    </string-array>

    <!-- Run modes for client (display) -->
    <string-array name="clientModeRun">
        <item>Toujours en fonctionnement</item>
        <item>Fonctionnement basé sur les préférences</item>
        <item>Suspendu</item>
    </string-array>
    <!-- Network modes for client (display) -->
    <string-array name="clientModeNetwork">
        <item>Activité réseau toujours disponible</item>
        <item>Activité réseau basée sur les préférences</item>
        <item>Activité réseau suspendue</item>
    </string-array>
    <!-- GPU modes for client (display) -->
    <string-array name="clientModeGpu">
        <item>GPU toujours disponible</item>
        <item>GPU basé sur les préférences</item>
        <item>GPU suspendu</item>
    </string-array>
    <!-- Modes for client (values) - both run and network -->
    <string-array name="clientModeValues">
        <item>1</item>
        <item>2</item>
        <item>3</item>
    </string-array>

    <!-- States of task (all tasks) -->
    <string-array name="resultStates">
        <item>Nouveau</item> <!-- 0 RESULT_NEW -->
        <item>En téléchargement</item> <!-- 1 RESULT_FILES_DOWNLOADING -->
        <item>Téléchargé</item> <!-- 2 RESULT_FILES_DOWNLOADED -->
        <item>Erreur de calcul</item> <!-- 3 RESULT_COMPUTE_ERROR -->
        <item>En envoi</item> <!-- 4 RESULT_FILES_UPLOADING -->
        <item>Prêt pour l\'envoi</item> <!-- 5 RESULT_FILES_UPLOADED -->
        <item>Abandonné</item> <!-- 6 RESULT_ABORTED -->
    </string-array>
    <!-- States of task (active tasks only) -->
    <string-array name="activeTaskStates">
        <item>Prêt à commencer</item> <!-- 0 PROCESS_UNINITIALIZED (process doesn't exist yet) -->
        <item>En calcul</item> <!-- 1 PROCESS_EXECUTING (process is running, as far as we know) -->
        <item>Sorti</item> <!-- 2 *PROCESS_EXITED -->
        <item>Sorti</item> <!-- 3 *PROCESS_WAS_SIGNALED -->
        <item>Sorti</item> <!-- 4 *PROCESS_EXIT_UNKNOWN -->
        <item>Abandonné</item> <!-- 5 PROCESS_ABORT_PENDING (process exceeded limits; send "abort" message, waiting to exit) -->
        <item>Abandonné</item> <!-- 6 *PROCESS_ABORTED -->
        <item>Départ annulé</item> <!-- 7 *PROCESS_COULDNT_START -->
        <item>Sortir</item> <!-- 8 PROCESS_QUIT_PENDING (we've sent it a "quit" message, waiting to exit) -->
        <item>Attente de démarrage</item> <!-- 9 PROCESS_SUSPENDED (we've sent it a "suspend" message) -->
    </string-array>
    <string-array name="pollOps">
        <item>Rattacher a BAM</item>
        <item>Synchroniser avec BAM</item>
        <item>Voir compte de projet</item>
        <item>Créer un compte de projet</item>
        <item>Rattacher projet</item>
        <item>Configuration projet</item>
    </string-array>
    <string-array name="messageFilterLabels">
        <item>Filtre: Inconnu</item>
        <item>Filtre: Info</item>
        <item>Filtre: Alerte utilisateur</item>
        <item>Filtre: Erreur interne</item>
    </string-array>
    <string-array name="messageFilterMenuItems">
        <item>Inconnu</item>
        <item>Info</item>
        <item>Alerte utilisateur</item>
        <item>Erreur interne</item>
    </string-array>
    <string-array name="nativeAutostartLabels">
        <item>Pas de démarrage auto</item>
        <item>Au démarrage de l\'application</item>
        <item>Au démarrage système</item>
        <item>Démarrage auto en permanence</item>
    </string-array>
    <string-array name="nativeAutostartValues">
        <item>0</item>
        <item>1</item>
        <item>2</item>
        <item>3</item>
    </string-array>
    <string-array name="placesToInstallLabels">
        <item>Mémoire interne</item>
        <item>Mémoire SDCard</item>
    </string-array>

</resources>