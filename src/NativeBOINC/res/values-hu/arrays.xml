	<!-- States of task (all tasks) -->
	<string-array name="resultStates">
		<item>Új</item>                 <!-- 0 RESULT_NEW               -->
		<item>Letöltés</item>         <!-- 1 RESULT_FILES_DOWNLOADING -->
		<item>Letöltve</item>          <!-- 2 RESULT_FILES_DOWNLOADED  -->
		<item>Számítási hiba</item>   <!-- 3 RESULT_COMPUTE_ERROR     -->
		<item>Feltöltés</item>           <!-- 4 RESULT_FILES_UPLOADING   -->
		<item>Kész van a visszaküldéshez!</item>     <!-- 5 RESULT_FILES_UPLOADED    -->
		<item>Megszakítva</item>             <!-- 6 RESULT_ABORTED           -->
	</string-array>
	<!-- States of task (active tasks only) -->
	<string-array name="activeTaskStates">
		<item>Kész van a starthoz!</item>  <!-- 0 PROCESS_UNINITIALIZED (process doesn't exist yet) -->
		<item>Fut...</item>         <!-- 1 PROCESS_EXECUTING (process is running, as far as we know) -->
		<item>Kilépett</item>          <!-- 2 *PROCESS_EXITED -->
		<item>Kilépett</item>          <!-- 3 *PROCESS_WAS_SIGNALED     -->
		<item>Kilépett</item>          <!-- 4 *PROCESS_EXIT_UNKNOWN   -->
		<item>Megszakítva</item>         <!-- 5 PROCESS_ABORT_PENDING (process exceeded limits; send "abort" message, waiting to exit)   -->
		<item>Megszakítva</item>         <!-- 6 *PROCESS_ABORTED          -->
		<item>Hiba a feladat elkezdésekor</item> <!-- 7 *PROCESS_COULDNT_START   -->
		<item>Kilépés</item>         <!-- 8 PROCESS_QUIT_PENDING (we've sent it a "quit" message, waiting to exit) -->
		<item>Várakozás a futásra</item>  <!-- 9 PROCESS_SUSPENDED (we've sent it a "suspend" message)        -->
	</string-array>

